<?xml version="1.0"?>
<s:VGroup xmlns:fx="http://ns.adobe.com/mxml/2009"
		  xmlns:s="library://ns.adobe.com/flex/spark"
		  width="100%"
		  gap="0">
	<s:Line width="100%">
		<s:stroke>
			<s:SolidColorStroke id="lineColor"
								color="#2D7EBA"/>
		</s:stroke>
	</s:Line>
	<s:Line width="100%">
		<s:stroke>
			<s:SolidColorStroke id="shadowColor"
								color="#FFFFFF"
								alpha=".5"/>
		</s:stroke>
	</s:Line>
</s:VGroup>
