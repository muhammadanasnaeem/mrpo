<?xml version="1.0" encoding="utf-8"?>
<s:Skin name="CustomApplicationSkin"
		xmlns:fx="http://ns.adobe.com/mxml/2009"
		xmlns:s="library://ns.adobe.com/flex/spark"
		alpha.disabled="0.5"
		width="64"
		height="17">
	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>

	<fx:Metadata>
		<![CDATA[ 
		[HostComponent("spark.components.Button")]
		]]>
	</fx:Metadata>
	<s:Group id="backgroundRect">
		<s:BitmapImage source="@Embed('../../../images/buyok.png')"
					   left="0"
					   right="0"
					   top="0"
					   bottom="0"
					   scaleMode="stretch"/>
	</s:Group>
</s:Skin>
